{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",

  "IdentityServer": {
    "Clients": {
      "{APP ASSEMBLY}.Client": {
        "Profile": "IdentityServerSPA"
      }
    }
  },
  "JWTSettings": {
    "securityKey": "SecretKeyWhichIsNotReallySecret",
    "validIssuer": "BlazorWASM",
    "validAudience": "https://localhost:5010",
    "expiryInMinutes": 5
  }
}
